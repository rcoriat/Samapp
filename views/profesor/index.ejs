<div class="container">
        <h2>Profesores registrados</h2>
        <a class="button" href="/profesor/new"> Agregar profesor </a>
        <a class="button" href="/"> Regresar </a>
    
        <table class="table table-hover">
            <th>Cedula</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th colspan="2" class="text-center">Acciones</th>
            <%_.each(profesor,function(profesor){ %>
            <tr>
                <td><%=profesor.cedula%></td>
                <td><%=profesor.nombre%></td>
                <td><%=profesor.apellido%></td>
        
                <td>
                    <a href="/profesor/show/<%=profesor.id%>" class="btn btn-success btn-sm "> Ver Detalles</a>
                </td>
                <td>
                    <form action="/profesor/destroy/<%=profesor.id%>" method="POST" accept-charset="utf-8">
                        <input type="hidden" name="method" value="delete"/>
                        <input type="submit" class="btn btn-sm btn-danger" value="Eliminar"/>
                        <input type="hidden" name="_csrf" value="<%=_csrf%>"/>
    
                    </form>
                        
                
                </td>
            </tr>
    
        <%})%>
    
        </table>
    </div>